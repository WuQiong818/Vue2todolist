r\n        this.currentForm = form;\r\n      },\r\n      refreshCode() {\r\n        this.codeImgSrc = `${this.$pageContext.servletContext.contextPath}/AuthCode/code?s=${new Date().getTime()}`;\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .login-sign {\r\n    width: 100%;\r\n    height: 100%;\r\n    border: 1px #056768 solid;\r\n    border-radius: 10px;\r\n  \r\n    .container {\r\n      float: left;\r\n      overflow: hidden;\r\n    }\r\n  \r\n    .a-container {\r\n      width: calc(20% - 10px);\r\n      height: 100%;\r\n      background: #a0cfff;\r\n      border-radius: 10px 0 0 10px;\r\n    }\r\n  \r\n    .b-container {\r\n      width: 80%;\r\n      height: 100%;\r\n  \r\n      .switch {\r\n        .switch_container {\r\n          h2 {\r\n            letter-spacing: 0;\r\n          }\r\n  \r\n          .switch-title {\r\n            font-size: 13px;\r\n          }\r\n        }\r\n  \r\n        .switch_button {\r\n          width: 267px;\r\n        }\r\n  \r\n        .switch_circle {\r\n          width: 80px;\r\n          height: 60px;\r\n        }\r\n      }\r\n    }\r\n  </style>\r\n  "],"mappings":"AAkNA;EACAA,KAAA;IACA;MACAC,QAAA;MACAC,QAAA;MACAC,SAAA;MACAC,aAAA;MACAC,aAAA;MACAC,gBAAA;MACAC,UAAA;MACAC,WAAA;MACAC,UAAA,UAAAC,YAAA,CAAAC,cAAA,CAAAC,WAAA;IACA;EACA;EACAC,OAAA;IACAC,OAAA;MACA;MACA;IAAA,CACA;IACAC,MAAA;MACA;MACA;IAAA,CACA;IACAC,WAAAC,IAAA;MACA,KAAAT,WAAA,GAAAS,IAAA;IACA;IACAC,YAAA;MACA,KAAAT,UAAA,WAAAC,YAAA,CAAAC,cAAA,CAAAC,WAAA,wBAAAO,IAAA,GAAAC,OAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}