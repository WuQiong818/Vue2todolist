{"ast":null,"code":"//导入draggable组件\nimport draggable from \"vuedraggable\";\nimport { mapGetters, mapState } from \"vuex\";\nexport default {\n  //注册draggable组件\n  components: {\n    draggable\n  },\n  data() {\n    return {\n      //定义要被拖拽对象的数组\n      finishedTask: JSON.parse(localStorage.getItem(\"finishedTask\")) || [{\n        id: \"1\",\n        name: \"待分组任务\"\n      }],\n      arr1: JSON.parse(localStorage.getItem(\"oneArea\")) || [{\n        id: \"1\",\n        name: \"重要紧急\"\n      }, {\n        id: \"2\",\n        name: \"占位元素,保证数据不为空\"\n      }],\n      arr2: JSON.parse(localStorage.getItem(\"twoArea\")) || [{\n        id: \"1\",\n        name: \"重要不紧急\"\n      }],\n      arr3: JSON.parse(localStorage.getItem(\"threeArea\")) || [{\n        id: \"1\",\n        name: \"紧急不重要\"\n      }],\n      arr4: JSON.parse(localStorage.getItem(\"fourArea\")) || [{\n        id: \"1\",\n        name: \"不重要不紧急\"\n      }],\n      displayObj: {\n        visibility: 'hidden'\n      },\n      displayNormal: {}\n    };\n  },\n  methods: {\n    //move回调方法\n    onMove(e, originalEvent) {\n      //不允许停靠\n      if (e.relatedContext.element.id == 1) return false;\n      //不允许拖拽\n      if (e.draggedContext.element.id == 1) return false;\n      return true;\n    },\n    open() {\n      this.$notify({\n        title: \"提醒\",\n        message: \"只有已完成的任务才可以进行复盘归类\",\n        offset: 100\n      });\n    }\n  },\n  computed: {\n    ...mapState(\"todolist\", [\"todos\"]),\n    getFinishedTodos: {\n      get() {\n        return this.todos.filter(item => {\n          return item.isFinished == true;\n        });\n      },\n      set() {\n        console.log(\"set调用了\");\n      }\n    }\n  },\n  watch: {\n    finishedTask: {\n      deep: true,\n      handler(newValue) {\n        localStorage.setItem(\"finishedTask\", JSON.stringify(newValue));\n        console.log(\"finishedTask\");\n      }\n    },\n    arr1: {\n      deep: true,\n      handler(newValue) {\n        localStorage.setItem(\"oneArea\", JSON.stringify(newValue));\n        console.log(\"oneArea\");\n      }\n    },\n    arr2: {\n      deep: true,\n      handler(newValue) {\n        localStorage.setItem(\"twoArea\", JSON.stringify(newValue));\n        console.log(\"twoArea\");\n      }\n    },\n    arr3: {\n      deep: true,\n      handler(newValue) {\n        localStorage.setItem(\"threeArea\", JSON.stringify(newValue));\n        console.log(\"Three\");\n      }\n    },\n    arr4: {\n      deep: true,\n      handler(newValue) {\n        localStorage.setItem(\"fourArea\", JSON.stringify(newValue));\n        console.log(\"Four\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["draggable","mapGetters","mapState","components","data","finishedTask","JSON","parse","localStorage","getItem","id","name","arr1","arr2","arr3","arr4","displayObj","visibility","displayNormal","methods","onMove","e","originalEvent","relatedContext","element","draggedContext","open","$notify","title","message","offset","computed","getFinishedTodos","get","todos","filter","item","isFinished","set","console","log","watch","deep","handler","newValue","setItem","stringify"],"sources":["src/pages/FourQuadrant.vue"],"sourcesContent":["<script>\r\n//导入draggable组件\r\nimport draggable from \"vuedraggable\";\r\nimport { mapGetters, mapState } from \"vuex\";\r\nexport default {\r\n  //注册draggable组件\r\n  components: {\r\n    draggable,\r\n  },\r\n  data() {\r\n    return {\r\n      //定义要被拖拽对象的数组\r\n      finishedTask: JSON.parse(localStorage.getItem(\"finishedTask\")) || [\r\n        { id: \"1\", name: \"待分组任务\" },\r\n      ],\r\n      arr1: JSON.parse(localStorage.getItem(\"oneArea\")) || [\r\n        { id: \"1\", name: \"重要紧急\" },\r\n        { id: \"2\", name: \"占位元素,保证数据不为空\" },\r\n      ],\r\n      arr2: JSON.parse(localStorage.getItem(\"twoArea\")) || [\r\n        { id: \"1\", name: \"重要不紧急\" },\r\n      ],\r\n      arr3: JSON.parse(localStorage.getItem(\"threeArea\")) || [\r\n        { id: \"1\", name: \"紧急不重要\" },\r\n      ],\r\n      arr4: JSON.parse(localStorage.getItem(\"fourArea\")) || [\r\n        { id: \"1\", name: \"不重要不紧急\" },\r\n      ],\r\n      displayObj:{\r\n        visibility: 'hidden'\r\n      },\r\n      displayNormal:{}\r\n    };\r\n  },\r\n  methods: {\r\n    //move回调方法\r\n    onMove(e, originalEvent) {\r\n      //不允许停靠\r\n      if (e.relatedContext.element.id == 1) return false;\r\n      //不允许拖拽\r\n      if (e.draggedContext.element.id == 1) return false;\r\n      return true;\r\n    },\r\n    open() {\r\n      this.$notify({\r\n        title: \"提醒\",\r\n        message: \"只有已完成的任务才可以进行复盘归类\",\r\n        offset: 100,\r\n      });\r\n    },\r\n  },\r\n\r\n  computed: {\r\n    ...mapState(\"todolist\", [\"todos\"]),\r\n    getFinishedTodos: {\r\n      get() {\r\n        return this.todos.filter((item) => {\r\n          return item.isFinished == true;\r\n        });\r\n      },\r\n      set() {\r\n        console.log(\"set调用了\");\r\n      },\r\n    },\r\n  },\r\n  watch: {\r\n    finishedTask: {\r\n      deep: true,\r\n      handler(newValue) {\r\n        localStorage.setItem(\"finishedTask\", JSON.stringify(newValue));\r\n        console.log(\"finishedTask\");\r\n      },\r\n    },\r\n    arr1: {\r\n      deep: true,\r\n      handler(newValue) {\r\n        localStorage.setItem(\"oneArea\", JSON.stringify(newValue));\r\n        console.log(\"oneArea\");\r\n      },\r\n    },\r\n    arr2: {\r\n      deep: true,\r\n      handler(newValue) {\r\n        localStorage.setItem(\"twoArea\", JSON.stringify(newValue));\r\n        console.log(\"twoArea\");\r\n      },\r\n    },\r\n    arr3: {\r\n      deep: true,\r\n      handler(newValue) {\r\n        localStorage.setItem(\"threeArea\", JSON.stringify(newValue));\r\n        console.log(\"Three\");\r\n      },\r\n    },\r\n    arr4: {\r\n      deep: true,\r\n      handler(newValue) {\r\n        localStorage.setItem(\"fourArea\", JSON.stringify(newValue));\r\n        console.log(\"Four\");\r\n      },\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<template>\r\n  <div>\r\n    <!--使用draggable组件-->\r\n    <div class=\"main\">\r\n      <div class=\"header\">\r\n        <h1 style=\"font-weight: 600\">\r\n          创造新大陆的,不是那滚滚的波涛,却是它底下的泥沙.\r\n        </h1>\r\n        <el-button plain @click=\"open\"> 提示 </el-button>\r\n      </div>\r\n      <div class=\"task\">\r\n        <draggable\r\n          v-model=\"finishedTask\"\r\n          group=\"itxst\"\r\n          ghostClass=\"ghost\"\r\n          chosenClass=\"chosen\"\r\n          filter=\".forbid\"\r\n          animation=\"300\"\r\n          :move=\"onMove\"\r\n        >\r\n          <transition-group>\r\n            <div\r\n              :class=\"item.id == '1' ? 'item forbid' : 'item'\"\r\n              :style=\"item.id == '' ? displayObj : displayNormal\" \r\n              v-for=\"item in finishedTask\"\r\n              :key=\"item.id\"\r\n            >\r\n              {{ item.name }}\r\n            </div>\r\n          </transition-group>\r\n        </draggable>\r\n      </div>\r\n      <div class=\"subarea\">\r\n        <div class=\"col\">\r\n          <draggable\r\n            v-model=\"arr1\"\r\n            group=\"itxst\"\r\n            ghostClass=\"ghost\"\r\n            chosenClass=\"chosen\"\r\n            filter=\".forbid\"\r\n            animation=\"300\"\r\n            :move=\"onMove\"\r\n          >\r\n          <!-- :style=\"item.id == '2' ? '{display:none}' : '{display:block}'\" -->\r\n            <transition-group>\r\n              <div\r\n                :class=\"item.id == 1 ? 'item forbid' : 'item'\"\r\n                :style=\"item.id == '2' ? displayObj : displayNormal\" \r\n                v-for=\"item in arr1\"\r\n                :key=\"item.id\"\r\n              >\r\n                {{ item.name }}-{{item.id}}\r\n              </div>\r\n            </transition-group>\r\n          </draggable>\r\n        </div>\r\n\r\n        <div class=\"col\">\r\n          <draggable\r\n            v-model=\"arr2\"\r\n            group=\"itxst\"\r\n            ghostClass=\"ghost\"\r\n            chosenClass=\"chosen\"\r\n            filter=\".forbid\"\r\n            animation=\"300\"\r\n            :move=\"onMove\"\r\n          >\r\n            <transition-group>\r\n              <div\r\n                :class=\"item.id == 1 ? 'item forbid' : 'item'\"\r\n                v-for=\"item in arr2\"\r\n                :key=\"item.id\"\r\n              >\r\n                {{ item.name }}\r\n              </div>\r\n            </transition-group>\r\n          </draggable>\r\n        </div>\r\n        <div class=\"col\">\r\n          <draggable\r\n            v-model=\"arr3\"\r\n            group=\"itxst\"\r\n            ghostClass=\"ghost\"\r\n            chosenClass=\"chosen\"\r\n            filter=\".forbid\"\r\n            animation=\"300\"\r\n            :move=\"onMove\"\r\n          >\r\n            <transition-group>\r\n              <div\r\n                :class=\"item.id == 1 ? 'item forbid' : 'item'\"\r\n                v-for=\"item in arr3\"\r\n                :key=\"item.id\"\r\n              >\r\n                {{ item.name }}\r\n              </div>\r\n            </transition-group>\r\n          </draggable>\r\n        </div>\r\n        <div class=\"col\">\r\n          <draggable\r\n            v-model=\"arr4\"\r\n            group=\"itxst\"\r\n            ghostClass=\"ghost\"\r\n            chosenClass=\"chosen\"\r\n            filter=\".forbid\"\r\n            animation=\"300\"\r\n            :move=\"onMove\"\r\n          >\r\n            <transition-group>\r\n              <div\r\n                :class=\"item.id == 1 ? 'item forbid' : 'item'\"\r\n                v-for=\"item in arr4\"\r\n                :key=\"item.id\"\r\n              >\r\n                {{ item.name }}\r\n              </div>\r\n            </transition-group>\r\n          </draggable>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n.header {\r\n  position: relative;\r\n}\r\n.header button {\r\n  position: absolute;\r\n  top: -3px;\r\n  right: 57px;\r\n}\r\n.main {\r\n  margin: 10px;\r\n}\r\n\r\n.col,\r\n.task {\r\n  flex: 1;\r\n  padding: 10px;\r\n  border: solid 1px #eee;\r\n  border-radius: 5px;\r\n  float: left;\r\n  overflow: auto;\r\n}\r\n.task {\r\n  margin-top: 15px;\r\n  width: 20vw;\r\n  height: 86.9vh;\r\n  overflow: auto;\r\n}\r\n.subarea {\r\n  margin: 10px;\r\n  float: left;\r\n  width: 60vw;\r\n  height: 90vh;\r\n  overflow: hidden;\r\n}\r\n.subarea .col {\r\n  width: 28vw;\r\n  height: 41.5vh;\r\n  float: left;\r\n  margin: 5px;\r\n}\r\n\r\n.drag {\r\n  background-color: blue !important;\r\n  border: solid 3px red;\r\n}\r\n.chosen {\r\n  background-color: #000 !important;\r\n  color: #fff;\r\n}\r\n.ghost {\r\n  background-color: red !important;\r\n}\r\n\r\n.item {\r\n  padding: 6px 12px;\r\n  margin: 0px 10px 0px 10px;\r\n  border: solid 1px #eee;\r\n  background-color: #f1f1f1;\r\n  text-align: left;\r\n}\r\n.item + .item {\r\n  border-top: none;\r\n  margin-top: 6px;\r\n}\r\n.item:hover {\r\n  background-color: #fdfdfd;\r\n  cursor: move;\r\n}\r\n</style>"],"mappings":"AACA;AACA,OAAAA,SAAA;AACA,SAAAC,UAAA,EAAAC,QAAA;AACA;EACA;EACAC,UAAA;IACAH;EACA;EACAI,KAAA;IACA;MACA;MACAC,YAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA,sBACA;QAAAC,EAAA;QAAAC,IAAA;MAAA,EACA;MACAC,IAAA,EAAAN,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA,iBACA;QAAAC,EAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,EAAA;QAAAC,IAAA;MAAA,EACA;MACAE,IAAA,EAAAP,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA,iBACA;QAAAC,EAAA;QAAAC,IAAA;MAAA,EACA;MACAG,IAAA,EAAAR,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA,mBACA;QAAAC,EAAA;QAAAC,IAAA;MAAA,EACA;MACAI,IAAA,EAAAT,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA,kBACA;QAAAC,EAAA;QAAAC,IAAA;MAAA,EACA;MACAK,UAAA;QACAC,UAAA;MACA;MACAC,aAAA;IACA;EACA;EACAC,OAAA;IACA;IACAC,OAAAC,CAAA,EAAAC,aAAA;MACA;MACA,IAAAD,CAAA,CAAAE,cAAA,CAAAC,OAAA,CAAAd,EAAA;MACA;MACA,IAAAW,CAAA,CAAAI,cAAA,CAAAD,OAAA,CAAAd,EAAA;MACA;IACA;IACAgB,KAAA;MACA,KAAAC,OAAA;QACAC,KAAA;QA