{"ast":null,"code":"import DrawFooter from \"@/components/DrawFooter.vue\";\nimport { mapMutations } from \"vuex\";\nexport default {\n  name: \"DrawTodo\",\n  components: {\n    DrawFooter\n  },\n  props: [\"todoObj\"],\n  data() {\n    return {\n      deadlineTime: this.todoObj.todoThing.deadline\n    };\n  },\n  methods: {\n    ...mapMutations({\n      DELETEDSTEPS: \"todolist/DELETEDSTEPS\"\n    }, {\n      SETDEADLINE: \"todolist/SETDEADLINE\"\n    }),\n    deletedSteps(index) {\n      this.todoObj.todoThing.steps.splice(index, 1);\n      this.DELETEDSTEPS(this.todoObj);\n    },\n    deadline() {\n      this.todoObj.todoThing.deadline = this.deadlineTime;\n      this.SETDEADLINE(this.todoObj);\n    }\n  }\n};","map":{"version":3,"names":["DrawFooter","mapMutations","name","components","props","data","deadlineTime","todoObj","todoThing","deadline","methods","DELETEDSTEPS","SETDEADLINE","deletedSteps","index","steps","splice"],"sources":["src/components/DrawTodo.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"target\">\r\n      <input type=\"checkbox\" name=\"\" id=\"\" />\r\n      <span>总任务: {{ todoObj.todoThing.title }}</span>\r\n    </div>\r\n    <div class=\"steps\">\r\n      <div v-for=\"(item, index) in todoObj.todoThing.steps\" :key=\"index\">\r\n        <input type=\"checkbox\" />\r\n        <span>步骤{{ index + 1 }} - {{ item. }}</span>\r\n        <el-button\r\n          type=\"danger\"\r\n          icon=\"el-icon-delete\"\r\n          @click=\"deletedSteps(index)\"\r\n          circle\r\n        ></el-button>\r\n      </div>\r\n    </div>\r\n    <DrawFooter :todoObj=\"todoObj\" />\r\n    <div class=\"block\">\r\n      <span class=\"demonstration\">截止日期:</span>\r\n      <el-date-picker\r\n        v-model=\"deadlineTime\"\r\n        type=\"date\"\r\n        placeholder=\"选择日期\"\r\n        format=\"yyyy 年 MM 月 dd 日\"\r\n        value-format=\"timestamp\"\r\n        @change=\"deadline\"\r\n      >\r\n      </el-date-picker>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport DrawFooter from \"@/components/DrawFooter.vue\";\r\nimport { mapMutations } from \"vuex\";\r\nexport default {\r\n  name: \"DrawTodo\",\r\n  components: {\r\n    DrawFooter,\r\n  },\r\n  props: [\"todoObj\"],\r\n  data() {\r\n    return {\r\n      deadlineTime: this.todoObj.todoThing.deadline,\r\n    };\r\n  },\r\n  methods: {\r\n    ...mapMutations(\r\n      { DELETEDSTEPS: \"todolist/DELETEDSTEPS\" },\r\n      { SETDEADLINE: \"todolist/SETDEADLINE\" }\r\n    ),\r\n    deletedSteps(index) {\r\n      this.todoObj.todoThing.steps.splice(index, 1);\r\n      this.DELETEDSTEPS(this.todoObj);\r\n    },\r\n    deadline() {\r\n      this.todoObj.todoThing.deadline = this.deadlineTime;\r\n      this.SETDEADLINE(this.todoObj);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.target {\r\n  margin-left: 10px;\r\n  font-size: 26px;\r\n  vertical-align: baseline;\r\n}\r\n.target input {\r\n  width: 25px;\r\n  height: 25px;\r\n  margin-right: 10px;\r\n}\r\n.steps {\r\n  margin-left: 20px;\r\n  width: 400px;\r\n  overflow: hidden;\r\n}\r\n.steps div {\r\n  position: relative;\r\n}\r\n.steps input {\r\n  margin-right: 5px;\r\n}\r\n.steps span {\r\n  padding: 3px;\r\n}\r\n.steps .el-button.is-circle {\r\n  position: absolute;\r\n  right: 3px;\r\n  top: 11px;\r\n  padding: 6px;\r\n}\r\n/* ------------------------ */\r\n.demonstration {\r\n  margin: 0px 15px;\r\n}\r\n</style>"],"mappings":"AAmCA,OAAAA,UAAA;AACA,SAAAC,YAAA;AACA;EACAC,IAAA;EACAC,UAAA;IACAH;EACA;EACAI,KAAA;EACAC,KAAA;IACA;MACAC,YAAA,OAAAC,OAAA,CAAAC,SAAA,CAAAC;IACA;EACA;EACAC,OAAA;IACA,GAAAT,YAAA,CACA;MAAAU,YAAA;IAAA,GACA;MAAAC,WAAA;IAAA,CACA;IACAC,aAAAC,KAAA;MACA,KAAAP,OAAA,CAAAC,SAAA,CAAAO,KAAA,CAAAC,MAAA,CAAAF,KAAA;MACA,KAAAH,YAAA,MAAAJ,OAAA;IACA;IACAE,SAAA;MACA,KAAAF,OAAA,CAAAC,SAAA,CAAAC,QAAA,QAAAH,YAAA;MACA,KAAAM,WAAA,MAAAL,OAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}